<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="rootPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.makar.simplexsolution.control.WindowController">
    <Button fx:id="helpButton" focusTraversable="false" mnemonicParsing="false" styleClass="helpButton" text="?"
            AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0"/>
    <VBox layoutX="15.0" layoutY="15.0" spacing="10.0" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0">
        <Label layoutX="8.0" layoutY="14.0" text="Уравнение:" AnchorPane.leftAnchor="15.0"
               AnchorPane.topAnchor="15.0"/>
        <TextField fx:id="equationField" layoutX="38.0" layoutY="57.0" prefHeight="15.0" prefWidth="170.0"
                   AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="35.0"/>
        <Label text="Ограничения:"/>
    </VBox>
    <Label layoutX="235.0" layoutY="14.0" text="Ответ:" AnchorPane.topAnchor="15.0"/>
    <VBox alignment="CENTER" layoutX="235.0" layoutY="39.0" spacing="15.0" AnchorPane.rightAnchor="15.0"
          AnchorPane.topAnchor="40.0">
        <TextArea fx:id="answerField" editable="false" focusTraversable="false" prefHeight="100.0" prefWidth="350.0"
                  styleClass="answerField" wrapText="true" VBox.vgrow="NEVER"/>
        <Button fx:id="solveButton" focusTraversable="false" mnemonicParsing="false" styleClass="solveButton"
                text="Решить"/>
    </VBox>
</AnchorPane>
